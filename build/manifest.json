{
    "name": "Smile New Tab",
    "description": "简单的修改默认的新建标签页",
    "version": "1.0.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "manifest_version": 2
}